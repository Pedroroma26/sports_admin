 <div class="container">

   <table class="table">

    <thead>
      <tr class="table_headers" id ="clubs_index">
        <th scope="">Name</th>
        <th scope="">Creation Date</th>
      </tr>
    </thead>
    <tbody>

      <% @competitions.each do |competititon| %>

      <tr>
        <td><%= link_to competititon.name, competition_path(competititon) %></td>
        <td><%= competititon.created_at.strftime('%d-%m-%Y') %></td>
      </tr>

      <% end %>
    </tbody>
  </table>

</div>
